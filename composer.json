{
    "name": "twogether/laravel-url-signer",
    "description": "Signs and validates URLs with public/private keys",
    "type": "library",
    "license": "MIT",

    "require": {
        "php": "^7.1|^8.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "predis/predis": "^1.1"
    },

    "autoload": {
        "psr-4": {
            "Twogether\\LaravelURLSigner\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Twogether\\LaravelURLSignerTests\\": "tests/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Twogether\\LaravelURLSigner\\URLSignerServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
